CLIENT_ID="your_xero_client_id_here"
CLIENT_SECRET="your_xero_client_secret_here"
REDIRECT_URI="https://127.0.0.1:8000/callback"
XERO_API_URL="https://api.xero.com"
XERO_TENANT_ID="your_xero_tenant_id_here"

# Stripe Configuration
STRIPE_API_KEY="sk_test_your_publishable_key_here"
STRIPE_SECRET_KEY="sk_test_your_secret_key_here"
STRIPE_WEBHOOK_SECRET="whsec_your_webhook_secret_here"

# Plaid Configuration
PLAID_CLIENT_ID="your_plaid_client_id_here"
PLAID_SECRET="your_plaid_secret_here"
PLAID_ENV="sandbox"

# Assistant Configuration
# Set to "openai", "gemini", or "llama32" to choose the assistant model
ASSISTANT_MODEL_TYPE=openai
USE_LLAMA32=false

# Google Gemini Configuration (required if ASSISTANT_MODEL_TYPE=gemini)
GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI Configuration (required if ASSISTANT_MODEL_TYPE=openai)
OPENAI_API_KEY=your_openai_api_key_here

# Llama 3.2 Configuration (required if ASSISTANT_MODEL_TYPE=llama32)
LLAMA_BASE_URL=http://localhost:11434/v1
LLAMA_MODEL=llama3.2

# Email Configuration (for alerts and notifications)
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SENDER_EMAIL=your_email@gmail.com
SENDER_PASSWORD=your_app_password_here

# Brevo transactional email / license delivery
BREVO_API_KEY=your_brevo_api_key_here
BREVO_SENDER_EMAIL=support@yourdomain.com
BREVO_SENDER_NAME=Financial Command Center

# MCP Server Configuration
MCP_SERVER_URL=https://127.0.0.1:8000
SSL_VERIFY=false

# License Server Configuration
LICENSE_SERVER="https://license.daywinlabs.com"
LICENSE_VERIFY_SSL="true"
LICENSE_AUTO_TLS="false"
LICENSE_TLS_CERT=""
LICENSE_TLS_KEY=""
LICENSE_TLS_CA=""
ADMIN_TOKEN="set_admin_token_here"

# Supabase configuration for installer distribution
SUPABASE_URL="https://yourproject.supabase.co"
SUPABASE_SERVICE_KEY="your_supabase_service_role_key_here"
SUPABASE_BUCKET="fcc-installers"
INSTALLER_FILE="FCC.zip"
SIGNED_URL_EXPIRY="86400"

# Optional support contact information
#SUPPORT_EMAIL=support@yourdomain.com
#INSTALLER_DOWNLOAD_URL=https://yourcdn.example.com/FCCInstaller.zip
